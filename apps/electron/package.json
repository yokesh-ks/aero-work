    "sharp": "^0.34.5",
    "vite": "npm:rolldown-vite@latest"
  },
  "dependencies": {
    "@aero-work/ui": "*",
    "@hookform/resolvers": "^3.10.0",
    "@octokit/rest": "^22.0.1",
